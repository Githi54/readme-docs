{
	"version": 3,
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/_image/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/download/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/validate/?$",
			"dest": "_render"
		},
		{
			"src": "^/docs/([^/]+?)/([^/]+?)/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/docs/([^/]+?)/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/guide/?$",
			"dest": "_render"
		}
	]
}